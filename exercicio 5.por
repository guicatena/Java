programa {
  funcao inicio() {
    
    real valor_compra, valor_prestacao

    escreva("digite o valor da compra: ")
    leia(valor_compra)

    escreva("digite a quantidade de prestações: ")
    leia(valor_prestacao)

    (valor_prestacao) * (valor_compra)

    escreva("valor da compra: ", valor_compra)
    escreva("quantidade de prestações: ", valor_prestacao)
  }
}
